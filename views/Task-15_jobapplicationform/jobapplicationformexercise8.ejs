                                                                                                                                                                                                                                                                                                                                                                                                                                                errorobject.expectedctc%>
                                                                                                                                                                                                                                                                                                                                                                                                                                          </span>
                                                                                                                                                                                                                                                                                                                                                                                                                                          <label
                                                                                                                                                                                                                                                                                                                                                                                                                                             for="currentctc">CurrentCTC</label>
                                                                                                                                                                                                                                                                                                                                                                                                                                          <input
                                                                                                                                                                                                                                                                                                                                                                                                                                             type="text"
                                                                                                                                                                                                                                                                                                                                                                                                                                             id="currentctc"
                                                                                                                                                                                                                                                                                                                                                                                                                                             name="currentctc"
                                                                                                                                                                                                                                                                                                                                                                                                                                             class="errorpreferance"
                                                                                                                                                                                                                                                                                                                                                                                                                                             value="<%= studentdata && studentdata.preferencedetails && studentdata.preferencedetails.currentctc ? studentdata.preferencedetails.currentctc : ''%>">
                                                                                                                                                                                                                                                                                                                                                                                                                                          <span
                                                                                                                                                                                                                                                                                                                                                                                                                                             class="errorpreferancestyle">
                                                                                                                                                                                                                                                                                                                                                                                                                                             <%=
                                                                                                                                                                                                                                                                                                                                                                                                                                                errorobject.currentctc%>
                                                                                                                                                                                                                                                                                                                                                                                                                                          </span>
                                                                                                                                                                                                                                                                                                                                                                                                                                       </td>
                                                                                                                                                                                                                                                                                                                                                                                                                                       <td>
                                                                                                                                                                                                                                                                                                                                                                                                                                          <label
                                                                                                                                                                                                                                                                                                                                                                                                                                             for="department">Department</label>
                                                                                                                                                                                                                                                                                                                                                                                                                                          <select
                                                                                                                                                                                                                                                                                                                                                                                                                                             id="department"
                                                                                                                                                                                                                                                                                                                                                                                                                                             name="department"
                                                                                                                                                                                                                                                                                                                                                                                                                                             class="preferancedropdown">
                                                                                                                                                                                                                                                                                                                                                                                                                                             <option
                                                                                                                                                                                                                                                                                                                                                                                                                                                value="">
                                                                                                                                                                                                                                                                                                                                                                                                                                                Select
                                                                                                                                                                                                                                                                                                                                                                                                                                                Department
                                                                                                                                                                                                                                                                                                                                                                                                                                             </option>
                                                                                                                                                                                                                                                                                                                                                                                                                                             <% for(let
                                                                                                                                                                                                                                                                                                                                                                                                                                                department
                                                                                                                                                                                                                                                                                                                                                                                                                                                of
                                                                                                                                                                                                                                                                                                                                                                                                                                                departmentarray){%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                <!-- used for insert -->
                                                                                                                                                                                                                                                                                                                                                                                                                                                <% departmentindex=departmentindex
                                                                                                                                                                                                                                                                                                                                                                                                                                                   +
                                                                                                                                                                                                                                                                                                                                                                                                                                                   1%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                   <% if(
                                                                                                                                                                                                                                                                                                                                                                                                                                                      datafrompostrquest
                                                                                                                                                                                                                                                                                                                                                                                                                                                      &&
                                                                                                                                                                                                                                                                                                                                                                                                                                                      Object.keys(datafrompostrquest).length
                                                                                                                                                                                                                                                                                                                                                                                                                                                      !==0
                                                                                                                                                                                                                                                                                                                                                                                                                                                      &&
                                                                                                                                                                                                                                                                                                                                                                                                                                                      datafrompostrquest.department_id===department.departmentid){
                                                                                                                                                                                                                                                                                                                                                                                                                                                      %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                      <% departmentselected="selected"
                                                                                                                                                                                                                                                                                                                                                                                                                                                         %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                         <%}else{%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                            <% departmentselected=""
                                                                                                                                                                                                                                                                                                                                                                                                                                                               %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                               <%}%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                  <!-- used for update -->
                                                                                                                                                                                                                                                                                                                                                                                                                                                                  <%if(
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     studentdata
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     &&
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     studentdata.preferencedetails
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     &&
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     Object.keys(studentdata.preferencedetails).length
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     !==0
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     &&
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     studentdata.preferencedetails.departmentid===department.departmentid){%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     <% departmentselected="selected"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                        %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                        <%}else{%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                           <% departmentselected=""
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              <%}%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 <%=
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    departmentselected%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    <option
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       id="department<%= departmentindex%>"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       value="<%= department.department%>"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       <%=departmentselected%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       >
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       <%=
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          department.department%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    </option>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    <%}%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       <!-- 
                        <option id="department1" value="Development">Development</option>
                        <option id="department2" value="Design">Design</option>
                        <option id="department3" value="Marketing">Marketing</option> -->

                                                                                                                                                                                                                                                                                                                                                                                                                                          </select>
                                                                                                                                                                                                                                                                                                                                                                                                                                          <span
                                                                                                                                                                                                                                                                                                                                                                                                                                             class="preferancedropdownerrorstyle">
                                                                                                                                                                                                                                                                                                                                                                                                                                             <%=
                                                                                                                                                                                                                                                                                                                                                                                                                                                errorobject.preferancedepartmentdropdown%>
                                                                                                                                                                                                                                                                                                                                                                                                                                          </span>
                                                                                                                                                                                                                                                                                                                                                                                                                                       </td>
                                                                                                                                                                                                                                                                                                                                                                                                                                    </tr>
                                                                                                                                                                                                                                                                                                                                                                                                                                 </table>
                                                                                                                                                                                                                                                                                                                                                                                                                              </fieldset>
                                                                                                                                                                                                                                                                                                                                                                                                                              <div
                                                                                                                                                                                                                                                                                                                                                                                                                                 class="submitbuttondiv">
                                                                                                                                                                                                                                                                                                                                                                                                                                 <% if(jobapplicationformaction
                                                                                                                                                                                                                                                                                                                                                                                                                                    &&
                                                                                                                                                                                                                                                                                                                                                                                                                                    jobapplicationformaction.formsubmit
                                                                                                                                                                                                                                                                                                                                                                                                                                    &&
                                                                                                                                                                                                                                                                                                                                                                                                                                    jobapplicationformaction.formsubmit
                                                                                                                                                                                                                                                                                                                                                                                                                                    !==""
                                                                                                                                                                                                                                                                                                                                                                                                                                    &&
                                                                                                                                                                                                                                                                                                                                                                                                                                    jobapplicationformaction.formupdate===""
                                                                                                                                                                                                                                                                                                                                                                                                                                    ){%>
                                                                                                                                                                                                                                                                                                                                                                                                                                    <input
                                                                                                                                                                                                                                                                                                                                                                                                                                       type="submit"
                                                                                                                                                                                                                                                                                                                                                                                                                                       id="submit"
                                                                                                                                                                                                                                                                                                                                                                                                                                       name="submit"
                                                                                                                                                                                                                                                                                                                                                                                                                                       value="Submit">
                                                                                                                                                                                                                                                                                                                                                                                                                                    <input
                                                                                                                                                                                                                                                                                                                                                                                                                                       type="reset"
                                                                                                                                                                                                                                                                                                                                                                                                                                       id="reset"
                                                                                                                                                                                                                                                                                                                                                                                                                                       name="reset"
                                                                                                                                                                                                                                                                                                                                                                                                                                       value="Reset">
                                                                                                                                                                                                                                                                                                                                                                                                                                    <% let
                                                                                                                                                                                                                                                                                                                                                                                                                                       formsubmitstyleclass=""
                                                                                                                                                                                                                                                                                                                                                                                                                                       %>
                                                                                                                                                                                                                                                                                                                                                                                                                                       <% if(errorobject.formsubmitmessage
                                                                                                                                                                                                                                                                                                                                                                                                                                          !==undefined){%>
                                                                                                                                                                                                                                                                                                                                                                                                                                          <% formsubmitstyleclass="formsubmitmessage"
                                                                                                                                                                                                                                                                                                                                                                                                                                             %>
                                                                                                                                                                                                                                                                                                                                                                                                                                             <%}else{%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                <% formsubmitstyleclass="formsubmitmessagehide"
                                                                                                                                                                                                                                                                                                                                                                                                                                                   %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                   <%}%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                      <p
                                                                                                                                                                                                                                                                                                                                                                                                                                                         class="<%= formsubmitstyleclass%>">
                                                                                                                                                                                                                                                                                                                                                                                                                                                         <%=
                                                                                                                                                                                                                                                                                                                                                                                                                                                            errorobject.formsubmitmessage%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                      </p>
                                                                                                                                                                                                                                                                                                                                                                                                                                                      <%}else
                                                                                                                                                                                                                                                                                                                                                                                                                                                         if(jobapplicationformaction.formsubmit===""
                                                                                                                                                                                                                                                                                                                                                                                                                                                         &&
                                                                                                                                                                                                                                                                                                                                                                                                                                                         jobapplicationformaction
                                                                                                                                                                                                                                                                                                                                                                                                                                                         &&
                                                                                                                                                                                                                                                                                                                                                                                                                                                         jobapplicationformaction.formupdate
                                                                                                                                                                                                                                                                                                                                                                                                                                                         &&
                                                                                                                                                                                                                                                                                                                                                                                                                                                         jobapplicationformaction.formupdate
                                                                                                                                                                                                                                                                                                                                                                                                                                                         !==""
                                                                                                                                                                                                                                                                                                                                                                                                                                                         ){%>

                                                                                                                                                                                                                                                                                                                                                                                                                                                         <!-- update button show -->
                                                                                                                                                                                                                                                                                                                                                                                                                                                         <input
                                                                                                                                                                                                                                                                                                                                                                                                                                                            type="submit"
                                                                                                                                                                                                                                                                                                                                                                                                                                                            id="submit"
                                                                                                                                                                                                                                                                                                                                                                                                                                                            name="submit"
                                                                                                                                                                                                                                                                                                                                                                                                                                                            value="Update">
                                                                                                                                                                                                                                                                                                                                                                                                                                                         <!-- <input type="reset" id="reset" name="reset" value="Reset"> -->
                                                                                                                                                                                                                                                                                                                                                                                                                                                         <% let
                                                                                                                                                                                                                                                                                                                                                                                                                                                            formupdatestyleclass=""
                                                                                                                                                                                                                                                                                                                                                                                                                                                            %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                            <% if(errorobject.formupdatemessage
                                                                                                                                                                                                                                                                                                                                                                                                                                                               !==undefined){%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                               <% formupdatestyleclass="formupdatemessage"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                  %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                  <%}else{%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                     <% formupdatestyleclass="formupdatemessagehide"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                        %>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                        <%}%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                           <p
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              class="<%= formupdatestyleclass%>">
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              <%=
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 errorobject.formupdatemessage%>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                           </p>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                           <%}%>
                                                                                                                                                                                                                                                                                                                                                                                                                              </div>
                                       </form>

